{
  "compilerOptions": {
    "lib": ["ES6", "ES2017", "DOM"],
    "target": "ES5",
    "jsx": "react-jsx",
    "allowJs": true,
    "outDir": "",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
